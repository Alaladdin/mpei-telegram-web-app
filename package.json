    "axios": "^0.27.2",
    "element-plus": "^2.2.6",
    "lodash": "^4.17.21",
    "marked": "^4.1.0",
    "moment": "^2.29.4",
    "pinia": "^2.0.14",
    "vue": "^3.2.37",
    "vue-router": "^4.0.16"
  },
  "devDependencies": {
    "@types/marked": "^4.0.7",
    "@types/node": "^16.11.41",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
