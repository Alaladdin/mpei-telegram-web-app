    "sass": "^1.53.0",
    "typescript": "~4.7.4",
    "unplugin-element-plus": "^0.4.0",
    "vite": "^2.9.12",
    "vite-plugin-imagemin": "^0.6.1",
    "vite-plugin-pwa": "^0.12.3",
